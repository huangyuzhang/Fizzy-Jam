{
    "layout": "single-tag-paged",
    "path": "tag",
    "permalink": "/tag/{{ slug }}/{% if pagination.pageNumber > 0 %}/page/{{ pagination.pageNumber + 1 }}{% endif %}/index.html",
    "pagination": {
        "data": "collections.post",
        "size": 2,
        "alias": "posts",
        "reverse": true
    },
    "tags": [
        "tag"
    ]
}